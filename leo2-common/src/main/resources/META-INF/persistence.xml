<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence">
    <persistence-unit name="leo2" transaction-type="RESOURCE_LOCAL">
        <class>org.deku.leo2.entities.v1.Depot</class>
        <properties>
            <property name="javax.persistence.jdbc.url"
                      value="jdbc:mysql://10.0.10.10:3306/dekuclient?zeroDateTimeBehavior=convertToNull"/>
            <property name="javax.persistence.jdbc.user" value="leo2"/>
            <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
            <property name="javax.persistence.jdbc.password" value="leo2"/>
            <property name="eclipselink.jdbc.url" value="jdbc:mysql://10.0.10.10:3306/dekuclient" />
            <property name="eclipselink.jdbc.driver" value="com.mysql.jdbc.Driver" />
        </properties>
    </persistence-unit>
</persistence>
