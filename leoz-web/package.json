{
  "name": "LeoZ",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "compile:electron:main": "tsc main.ts --outDir dist",
    "watch:electron:dev": "ng build --watch --base-href .",
    "build:electron:dev": "ng build --base-href . && compile:electron:main",
    "build:electron:dev:aot": "ng build --aot --base-href . && tsc main.ts --outDir dist",
    "build:electron:prod": "ng build --prod --aot --base-href . && tsc main.ts --outDir dist",
    "start": "npm run build:electron:dev && electron dist/main.js",
    "start:electron": "electron dist/main.js --serve",
    "start:web:dev": "http-server dist -c-1 -p 4200",
    "start:electron:dev": "npm-run-all compile:electron:main start:electron",
    "electron:serve": "npm run build:electron:prod && start:electron",
    "package:linux": "electron-packager . --overwrite --asar --platform=linux --arch=all --prune=true --out=release-builds",
    "package:windows": "electron-packager . --overwrite --asar --platform=win32 --arch=all --prune=true --out=release-builds",
    "package:mac": "electron-packager . --overwrite --asar --platform=darwin --arch=all --prune=true --out=release-builds",
    "electron:linux": "npm run build:electron:prod && npm run package:linux",
    "electron:windows": "npm run build:electron:prod && npm run package:windows",
    "electron:mac": "npm run build:electron:prod && npm run package:mac",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "main": "dist/main.js",
  "dependencies": {
    "@angular/animations": "5.0.3",
    "@angular/common": "5.0.3",
    "@angular/compiler": "5.0.3",
    "@angular/core": "5.0.3",
    "@angular/forms": "5.0.3",
    "@angular/http": "5.0.3",
    "@angular/platform-browser": "5.0.3",
    "@angular/platform-browser-dynamic": "5.0.3",
    "@angular/router": "5.0.3",
    "@yaga/leaflet-ng2": "1.0.0-rc8",
    "core-js": "2.5.1",
    "font-awesome": "4.7.0",
    "io-barcode": "1.3.0",
    "jspdf": "1.3.5",
    "moment": "2.18.1",
    "primeng": "5.0.0",
    "rxjs": "5.5.2",
    "web-animations-js": "2.3.1",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.5.4",
    "@angular/compiler-cli": "5.0.3",
    "@types/jasmine": "2.6.0",
    "@types/jspdf": "1.1.31",
    "@types/node": "8.0.31",
    "codelyzer": "3.2.0",
    "electron": "1.7.9",
    "electron-packager": "9.1.0",
    "electron-pdf-window": "^1.0.12",
    "electron-reload": "1.2.2",
    "http-server": "^0.10.0",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-phantomjs-launcher": "1.0.4",
    "npm-run-all": "4.1.2",
    "protractor": "5.1.2",
    "ts-node": "3.3.0",
    "tslint": "5.7.0",
    "typescript": "2.4.2"
  }
}
