  "scripts": {
    "ng": "ng",
    "docker:init": "npm run docker:build && npm run docker:run",
    "docker:build": "docker build -t derkurier/leoz .",
    "docker:run": "docker run -d -p 80:80 --name leoz derkurier/leoz",
    "docker:stop": "docker kill leoz",
    "docker:remove": "npm run docker:stop && docker rm leoz",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod --aot",
    "test": "ng test",
    "translate": "ng-xi18n --i18nFormat xlf",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@yaga/leaflet-ng2": "^1.0.0-rc5",
    "bootstrap-css-only": "^3.3.7",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "ngx-bootstrap": "^1.6.6",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.1",
